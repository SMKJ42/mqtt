[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = req_distinguished_name

[ req_distinguished_name ]
C                  = US
ST                 = <state>
L                  = <city>
O                  = <my_organization>
OU                 = <my_unit>
CN                 = 127.0.0.1